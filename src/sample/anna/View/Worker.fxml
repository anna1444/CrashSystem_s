            <Button fx:id="addButton" layoutX="265.0" layoutY="271.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="88.0" text="Добавить" />
            <TextField fx:id="nameField" layoutX="230.0" layoutY="64.0" prefHeight="25.0" prefWidth="140.0" promptText="Имя" />
            <Button fx:id="showButton" layoutX="158.0" layoutY="271.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="95.0" text="Просмотреть" />
            </children>
        </AnchorPane>
        <Text layoutX="151.0" layoutY="39.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Конфигурация &quot;Рабочий&quot;">
            <font>
                <Font size="23.0" />
            </font>
        </Text>
    </children>
</AnchorPane>
