                <TextField fx:id="lastnameField" layoutX="230.0" layoutY="24.0" prefHeight="25.0" prefWidth="140.0" promptText="Фамилия" />
                <Button fx:id="backButton" layoutX="31.0" layoutY="303.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="41.0" text="&lt;-" />
                <Button fx:id="changeButton" layoutX="370.0" layoutY="271.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="88.0" text="Изменить" />
                <Button fx:id="deleteButton" layoutX="476.0" layoutY="271.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="88.0" text="Удалить" />
            <TextField fx:id="surnameField" layoutX="230.0" layoutY="99.0" prefHeight="25.0" prefWidth="140.0" promptText="Отчество" />
