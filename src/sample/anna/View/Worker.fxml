<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.anna.Controllers.WorkerController">
    <children>
        <AnchorPane layoutY="45.0" prefHeight="354.0" prefWidth="600.0" style="-fx-background-color: #006363;">
