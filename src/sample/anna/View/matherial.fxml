                  <Font size="15.0" />
               </font>
            </Text>
            <TextField fx:id="nameField" layoutX="239.0" layoutY="57.0" prefHeight="25.0" prefWidth="140.0" promptText="Название материала" />
            <Button fx:id="backButton" layoutX="31.0" layoutY="303.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="41.0" text="&lt;-" />
            <TextField fx:id="priceField" layoutX="239.0" layoutY="102.0" prefHeight="25.0" prefWidth="140.0" promptText="Цена за ед(м)" />
